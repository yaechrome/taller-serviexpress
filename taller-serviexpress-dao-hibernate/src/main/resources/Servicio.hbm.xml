<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cl.taller.serviexpress.domain.Servicio" table="SERVICIO">
		<id name="id" column="ID" type="long">
			<generator class="native">
				<param name="sequence">ServicioSequence</param>
			</generator>
		</id>	
		<property name="estadoServicio"/>
		<property name="valor"/>
		<property name="descripcionServicio"/>					     
	</class>
</hibernate-mapping>